{
  "datasets": [
    {
      "name": "kitti_train",
      "type": "kitti",
      "data_dirs": [
        "/data2/wkl/pcc_data/sequences"
      ],
      "file_pattern": "**/velodyne/*.bin",
      "posQ": 4,
      "split_mode": "sequence_split",
